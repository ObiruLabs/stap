        {% block content %}
        {% endblock %}

        %footer#footer.container-fluid
            .row
                .col-sm-5
                    %a{ href: 'http://stapinc.org' } Southern Tier AIDS Program
                .col-sm-2
                    %a{ href: '/about' } About Us
                .col-sm-2
                    %a{ href: '/about#contact' } Contact
                .col-sm-3
                    %a{ href: '/policy' } Privacy Policy
